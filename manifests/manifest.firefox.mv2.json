{
  "manifest_version": 2,
  "name": "Multi-pass V2",
  "version": "0.2.0",
  "description": "Multi-pass to make HTTP basic auth easier.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background-mv2.js"
    ]
  },
  "browser_action": {
    "default_title": "Multi pass",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png"
    }
  },
  "options_page": "options.html",
  "applications": {
    "gecko": {
      "id": "stevereis93@gmail.com",
      "strict_min_version": "109.0"
    }
  }
}
